<div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label">
	<select class="mdl-textfield__input" id="{{key}}" name="{{key}}" {% if value.allowNull == false%} required {% endif %}>
		{% if value.allowNull == true%}
			<option disabled selected>Choose</option>
		{% endif %}
		<option value="true" {% if data == "true" %} selected{% endif %}>true</option>
		<option value="false" {% if data == "false" %} selected{% endif %}>false</option>
	</select>
	<label class="mdl-textfield__label" for="{{key}}">
    {{key}} 	:
	</label>
</div>
